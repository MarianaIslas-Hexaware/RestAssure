public class EcommerceAPITest {
	
	public static void main (String[]args) {
		
		RequestSpecification req= new RequestSpecBuilder().setBaseUri("https://rahulshettyacademy.com").setContentType(ContentType.JSON).build();
		given().spec(req).body()
	
	}

}
